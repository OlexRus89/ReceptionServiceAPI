			{
				"UrlName": "", 								// Наименование url-адреса
				"Url": "" 									// Url-адрес
			}
		],
		"StartupURL": "",	 								// Наименование запуска Url-адреса
		"ConnectionStrings": [ 								// Массив базы данных
			{												// Первый массив поддерживает PostgreSQL
				"Name": "", 								// Наименование базы данных
				"SQLName": "PostgreSQL",					// Тип подключения БД
				"ConnectionString": ""						// Путь подключения БД
			},
			{												// Второй массив поддерживает MS SQL
				"Name": "",									// Наименование базы данных
				"SQLName": "MSSQL",							// Тип подключения БД
				"ProviderName": "System.Data.SqlClient",	// Провайдер подключения
				"ConnectionString": ""						// Путь подключения БД
			}
		],
		"StartupSQLForAPI": "",								// Наименование запуска БД для API (Присваивается из ConnectionStrings)
